x = [3 1 5 7 9 2 6]

x =

     3     1     5     7     9     2     6

x(3)

ans =

     5

x(1:7)

ans =

     3     1     5     7     9     2     6

x(1:end)

ans =

     3     1     5     7     9     2     6

x(1:end-1)

ans =

     3     1     5     7     9     2

x(6:-2:1)

ans =

     2     7     1

x([1 6 2 1 1])

ans =

     3     2     1     3     3

sum(x)

ans =

    33

diary off
